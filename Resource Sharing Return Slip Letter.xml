<?xml version="1.0" encoding="utf-8"?>
<stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0">
	<include href="header.xsl" />
	<include href="senderReceiver.xsl" />
	<include href="mailReason.xsl" />
	<include href="footer.xsl" />
	<include href="style.xsl" />
	<include href="recordTitle.xsl" />
	<template match="/">
		<html>
			<if test="notification_data/languages/string">
				<attribute name="lang">
					<value-of select="notification_data/languages/string" />
				</attribute>
			</if>
			<head>
				<title>
					<value-of select="notification_data/general_data/subject" />
				</title>
				<call-template name="generalStyle" />
			</head>
			<body>
				<attribute name="style">
					<call-template name="bodyStyleCss" />
					<!-- style.xsl -->
				</attribute>
				<call-template name="head" />
				<div class="messageArea">
					<div class="messageBody">
						<table role="presentation" cellspacing="0" cellpadding="5" border="0">
							<attribute name="style">
								<call-template name="listStyleCss" />
								<!-- style.xsl -->
							</attribute>
							<tr>
								<td>
									<strong>
										@@returned@@
									</strong>
								</td>
							</tr>
							<tr>
								<td>
									<br />
									<strong>
										@@returned_to@@:
									</strong>
									<value-of select="notification_data/partner_name" />
								</td>
							</tr>
							<if test="notification_data/request/return_info !=''">
								<tr>
									<td>
										<value-of select="notification_data/request/return_info" />
									</td>
								</tr>
							</if>
							<tr>
								<td>
									<strong>
										@@address@@:
									</strong>
								</td>
							</tr>
							<if test="notification_data/partner_address/line1 !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/line1" />
									</td>
								</tr>
							</if>
							<if test="notification_data/partner_address/line2 !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/line2" />
									</td>
								</tr>
							</if>
							<if test="notification_data/partner_address/line3 !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/line3" />
									</td>
								</tr>
							</if>
							<if test="notification_data/partner_address/line4 !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/line4" />
									</td>
								</tr>
							</if>
							<if test="notification_data/partner_address/line5 !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/line5" />
									</td>
								</tr>
							</if>
							<if test="notification_data/partner_address/city !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/city" />
									</td>
								</tr>
							</if>
							<if test="notification_data/partner_address/country !=''">
								<tr>
									<td>
										<value-of select="notification_data/partner_address/country" />
									</td>
								</tr>
							</if>
							<tr>
								<td>
									<strong>
										@@email@@:
									</strong>
									<value-of select="notification_data/partner_email/email" />
								</td>
							</tr>
							<tr>
								<td>
									<strong>
										@@phone@@:
									</strong>
									<value-of select="notification_data/partner_phone/phone" />
								</td>
							</tr>
							<tr>
								<td>
									<strong>
										@@request_id@@:
									</strong>
									<img src="externalId.png" alt="externalId" />
								</td>
							</tr>
							<tr>
								<td>
									<br />
								</td>
							</tr>
							<if test="notification_data/request/display/title !=''">
								<tr>
									<td>
										<strong>
											@@title@@:
										</strong>
										<value-of select="notification_data/request/display/title" />
									</td>
								</tr>
							</if>
							<if test="notification_data/request/display/journal_title !=''">
								<tr>
									<td>
										<strong>
											@@journal_title@@:
										</strong>
										<value-of select="notification_data/request/display/journal_title" />
									</td>
								</tr>
							</if>
							<tr>
								<td>
									<strong>
										@@author@@:
									</strong>
									<value-of select="notification_data/request/display/author" />
								</td>
							</tr>
							<tr>
								<td>
									<strong>
										@@volume@@:
									</strong>
									<value-of select="notification_data/request/display/volume" />
								</td>
							</tr>
							<tr>
								<td>
									<strong>
										@@issue@@:
									</strong>
									<value-of select="notification_data/request/display/issue" />
								</td>
							</tr>
							<tr>
								<td>
									<br />
									<strong>
										@@arrival_date@@:
									</strong>
									<value-of select="notification_data/request/item_arrival_date" />
								</td>
							</tr>
							<tr>
								<td>
									<strong>
										@@required_return_date@@:
									</strong>
									<value-of select="notification_data/request/due_date" />
								</td>
							</tr>
							<tr>
								<td>
									<br />
									<strong>
										@@note_to_partner@@:
									</strong>
									<value-of select="notification_data/note_to_partner" />
								</td>
							</tr>
						</table>
						<br />
						<br />
						<table role="presentation">
							<tr>
								<td>
									@@signature@@
								</td>
							</tr>
							<tr>
								<td>
									<value-of select="notification_data/library/name" />
								</td>
							</tr>
							<if test="notification_data/library/address/line1 !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/line1" />
									</td>
								</tr>
							</if>
							<if test="notification_data/library/address/line2 !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/line2" />
									</td>
								</tr>
							</if>
							<if test="notification_data/library/address/line3 !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/line3" />
									</td>
								</tr>
							</if>
							<if test="notification_data/library/address/line4 !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/line4" />
									</td>
								</tr>
							</if>
							<if test="notification_data/library/address/line5 !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/line5" />
									</td>
								</tr>
							</if>
							<if test="notification_data/library/address/city !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/city" />
									</td>
								</tr>
							</if>
							<if test="notification_data/library/address/country !=''">
								<tr>
									<td>
										<value-of select="notification_data/library/address/country" />
									</td>
								</tr>
							</if>
						</table>
					</div>
				</div>
				<call-template name="lastFooter" />
				<!-- footer.xsl -->
			</body>
		</html>
	</template>
</stylesheet>
